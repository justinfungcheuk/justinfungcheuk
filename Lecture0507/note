// array.reduce()
[1,2,3].reduce(total, num) => total = total + num
// call back function 可以連續使用做加數


//Promise
//Promise 完成後，就會返回兩個可能性 - parameter
let complete = true;
let finishmeal = new Promise(function (resolve, reject) {
if (complete) {
resolve("I finished my meal within 30 minutes");
} else {
reject("I did not finish my meal within 30 minutes");
}
});

